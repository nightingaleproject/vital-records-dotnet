{
    "version": "0.2.0",
    "configurations": [

        {
            "name": ".NET Core Launch VRDR.CLI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-cli",
            "program": "${workspaceFolder}/projects/VRDR.CLI/bin/Debug/netcoreapp6.0/VRDR.CLI",
            "args": ["help"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}