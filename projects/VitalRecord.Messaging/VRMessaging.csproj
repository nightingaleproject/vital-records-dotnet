<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>VR</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>VR.Messaging</PackageId>
    <Title>FHIR VR Messaging</Title>
    <PackageDescription>A library for supporting FHIR messaging for VR FHIR records.</PackageDescription>
    <Authors>smacadam@mitre.org,rscalfani@mitre.org,pete@mitre.org</Authors>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Hl7.Fhir.R4" Version="5.5.1" />
